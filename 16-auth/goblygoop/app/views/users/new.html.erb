<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_for( @user ) do |u| %>
<%= u.text_field(:name, placeholder: "Enter Name") %>
<%= u.email_field(:email, placeholder: "Enter Email") %>
<%= u.password_field(:password, placeholder: "Password") %>
<%= u.password_field(:password_confirmation, placeholder: "check yo password") %>
<%= u.submit  %>
<% end %>
